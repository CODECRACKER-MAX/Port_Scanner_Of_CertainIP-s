import socket
s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
host = "50.62.160.26" # The host you want to scan, currently set to ablonepublic school's ip

def portScanner():
    for port in range(1,700): # Changing port value!
        print("Scanning Port, Please Wait...........")
        if s.connect_ex((host,port)):
            print("The Port: {} is Closed!".format(port))
        else:
            print("The Port: {} is Open!".format(port))
portScanner()
